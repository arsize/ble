page {
    background-color: #f5f5f5;
  }
  .allContent {
    width: 100%;
    height: 100vh;
  }
  .bottombar {
    width: 90%;
    position: absolute;
    left: 50%;
    transform: translate(-50%, -50%);
    bottom: 32rpx;
    display: flex;
    justify-content: space-between;
  }
  .bottombar .item {
    width: 327rpx;
    height: 100rpx;
    border-radius: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .bottombar .left {
    background-color: white;
    color: #1a6eff;
    font-size: 32rpx;
  }
  .bottombar .right {
    background: linear-gradient(270deg, #41beff 0%, #1a6eff 100%);
    color: #ffffff;
    box-shadow: 0rpx 4rpx 6rpx 0rpx rgba(27, 112, 255, 0.25);
  }
  .bottombar2 {
    width: 90%;
    position: absolute;
    left: 50%;
    transform: translate(-50%, -50%);
    bottom: 32rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
  }
  .bottombar2 .cancelMatch {
    width: 686rpx;
    height: 100rpx;
    background-color: white;
    border-radius: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #1a6eff;
    font-size: 32rpx;
    font-weight: bold;
  }
  .bottombar2 .text {
    font-size: 28rpx;
    color: #166cff;
    margin-bottom: 32rpx;
  }
  .blueconnectloading {
    width: 100%;
    height: 628rpx;
    background-color: white;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }
  .blueconnectloading .topblock {
    margin-top: 80rpx;
  }
  .blueconnectloading .subText {
    width: 622rpx;
    text-align: center;
    margin-top: 24rpx;
    margin-bottom: 50rpx;
  }
  .blueconnectloading .subText .subTextTop {
    font-size: 32rpx;
    color: #333333;
    font-weight: bold;
    margin-bottom: 32rpx;
  }
  .blueconnectloading .subText .subtitle {
    font-size: 28rpx;
    color: #333333;
  }
  