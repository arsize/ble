.home {
  width: 100%;
  height: 100rpx;
  text-align: center;
}
.tap {
  width: 200rpx;
  height: 100rpx;
  background-color: seagreen;
  border-radius: 15rpx;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  margin-top: 100rpx;
}
.tap2 {
  width: 200rpx;
  height: 100rpx;
  background-color: slateblue;
  border-radius: 15rpx;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  margin-top: 100rpx;
}
.btn {
  background-color: skyblue;
  padding: 10rpx;
  color: white;
  border-radius: 10rpx;
}
.blueline {
  width: 100%;
  box-sizing: border-box;
  margin-top: 50rpx;
  padding: 100rpx;
  display: flex;
  justify-content: space-between;
}
.bluespan {
  width: 100%;
  display: flex;
  box-sizing: border-box;
  justify-content: space-between;
  padding: 10rpx 100rpx;
}
.scrollline {
  margin-top: 30rpx;
  overflow-y: scroll;
  width: 600rpx;
  height: 300rpx;
  background-color: #f1f1f1;
  padding: 50rpx;
  box-sizing: border-box;
}
